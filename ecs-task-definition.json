{
    "family": "demo-exchange",
    "executionRoleArn": "arn:aws:iam::923061162103:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "demo-exchange",
            "image": "923061162103.dkr.ecr.us-east-2.amazonaws.com/demo-exchange:latest",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}